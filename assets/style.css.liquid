/* Large devices (laptops/desktops, 992px and up) */
@media only screen and (min-width: 992px) {

    .cate-item a,
    .nav-item a {
        position: relative;
    }

    .cate-item a span,
    .nav-item a span {
        opacity: 0.99;
    }

    .cate-item a:before,
    .nav-item a:before {
        content: '';
        position: absolute;
        left: -2px;
        bottom: 0;
        width: 0;
        height: 6px;
        border-radius: 3px;
        transition: width 0.25s ease-out;
    }

    .cate-item.on a:before,
    .nav-item.on a:before {
        width: calc(100% + 3px);
        background-image: linear-gradient(to bottom, #9bce26, #9bce26);
    }

    .util-area {
        display: flex;
        align-items: center;
    }

    .list-util {
        display: flex;
        margin-bottom: 0;
    }

    .list-util .util-item {
        width: 50px;
    }

    .list-util .util-item .link i svg {
        width: 100%;
        height: 32px;
        color: #3e3e3e;
    }

    .list-util .util-item .link .txt {
        display: block;
        font-size: .625rem;
        color: #141414;
        text-align: center;
    }

    .list-util .util-item .link i {
        width: 2rem;
        height: 2rem;
    }


    .header-nav-overlay {
        display: none !important;
    }

    .gnb {
        display: flex;
        justify-content: space-between;
        width: 1200px;
        margin: 0 auto;
    }

    .gnb .left {
        display: flex;
    }

    .header-top {
        display: flex;
        justify-content: space-between;
        width: 1200px;
        margin: 41px auto 28px;
    }

    .header-top .cus-col {
        display: flex;
    }

    .header-bottom {
        width: 100%;
    }

    .btn-cate {
        display: flex;
        cursor: pointer;
        background: transparent;
        border: none;
        outline: none;
        padding: 0;
        appearance: none;
        font-family: NotoSansJP, Roboto, Microsoft Yahei, \5b8b\9ad4, simsun, Arial, sans-serif;
    }

    .btn-cate .icon-hamburger {
        width: 20px;
        display: block;
    }

    .btn-cate .txt-menu {
        color: #141414;
        font-size: .9375rem;
        letter-spacing: 0;
        font-weight: bold;
        margin-left: 5px;
    }



    .btn-cate .icon-hamburger .line {
        width: 100%;
        height: 1px;
        background-color: #000;
        display: block;
        margin: 5px auto;
        -webkit-transition: all 0.3s ease-in-out;
        -o-transition: all 0.3s ease-in-out;
        transition: all 0.3s ease-in-out;
    }

    .btn-cate .icon-hamburger .line:first-child {
        margin-top: 4px;
    }

    .btn-cate .icon-hamburger .line:nth-child(3) {
        margin-bottom: 0;
    }

    .cus-col.menu .tit-cate-area {
        width: 400px;
    }

    .cus-col.trend .tit-cate-area {
        width: 200px;
    }

    .cus-col.banner {
        display: flex;
    }

    .tit-cate-area .txt-tit {
        color: #141414;
        font-size: .875rem;
        letter-spacing: 0;
        font-weight: 700;

    }

    .tit-cate-area .txt-tit:after {
        content: "";
        display: block;
        width: 100%;
        height: 1px;
        background: #cacaca;
    }
}

.wrap-gnb-container {
    background: #fff;
    display: none;
    width: 100%;
}

.cate .wrap-gnb-container .wrap-gnb-inner-container {
    display: flex;
