.btn-init {
    padding: 0;
    margin: 0;
    border: 0;
    background: transparent;
}

.search-area {
    display: flex;
    align-items: center;
}

.search-area .form-control {
    border-top-right-radius: 0.375rem !important;
    border-bottom-right-radius: 0.375rem !important;
    font-size: 0.8125rem;
}

.search-area .form-control:focus {
    background: transparent;
}

.search-area .btn-icon-search {
    position: absolute;
    padding: 4px 11px;
    right: 0;
    z-index: 20;
