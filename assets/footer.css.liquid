/* reset */

* {
  margin: 0;
  padding: 0;
}

body {
  font-family: "NotoSansJP", "Roboto", "Microsoft Yahei", '宋體', 'simsun',
    Arial, sans-serif;
  background-color: #f6f6f6;
}

ul,
li {
  list-style: none
}

a {
  text-decoration: none;
}

ul li a {
  color: #666;
}

/*// reset */

.footer {
  background: #f5f5f5;
}

/* .width { max-width: 1100px; margin: 0 auto; width: 90%; } */

.newsletter-area {}

.info-area {}

.navigation-area {}

.navigation-area .title,
.newsletter-area .title,
.info-area .title {
  margin-top: 0;
  margin-bottom: 0;
  font-size: 16px;
  color: inherit;
  color: #666;
}